{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json",
  },
  "extends": ["airbnb", "airbnb-typescript/base", "prettier"],
  "plugins": ["prettier", "@typescript-eslint"],
  "rules": {
    "prettier/prettier": ["error"],
    "no-param-reassign": ["warn"],
  },
  "globals": {
    "window": true,
    "document": true,
  },
}
